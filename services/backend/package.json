{
  "name": "myodyssey-backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.json && npx tsc-alias -p tsconfig.json",
    "start": "node dist/main.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev --name init"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "alith": "^0.9.0",
    "axios": "1.8.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-session": "^1.18.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "pg-boss": "^10.2.0",
    "sharp": "^0.34.1",
    "socket.io": "^4.8.1",
    "ethers": "^6.13.5",
    "tronweb": "^6.0.1"
  },
  "devDependencies": {
    "prisma": "^6.1.0",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.6.2",
    "tsc-alias": "^1.8.10"
  }
}